    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
